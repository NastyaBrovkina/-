@model IEnumerable<MvcApplication1.Models.Oplata>

@{
    ViewBag.Title = "Index";
}

<h2>Введите номер счета</h2>


<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Schet)
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Schet)
        </td>
        
       
    </tr>
}

</table>
